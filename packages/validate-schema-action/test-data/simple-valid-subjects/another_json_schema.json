{
  "$id": "another_json_schema",  
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Json Schema example object",
  "additionalProperties": false,
  "required": [
    "fraud_score"
  ],
  "properties": {
    "fraud_score": {
      "type": "number",
      "description": "Fraud score given by third party tools",
      "minimum": 0,
      "maximum": 1.0,
      "examples": [
        0.3, 0.1, 0.9995
      ]
    }
  }
} 